{
    "name": "devbaze/valet-win",
    "description": "Windows Subsystem for Linux.",
    "keywords": [
        "laravel",
        "zonda",
        "wwdhhd",
        "ubuntu",
        "fedora",
        "arch",
        "linux",
        "valet",
        "wsl",
        "laragon",
        "windows"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Ben"
        }
    ],
    "autoload": {
        "files": [
            "cli/includes/compatibility.php",
            "cli/includes/facades.php",
            "cli/includes/helpers.php"
        ],
        "psr-4": {
            "Valet\\": "cli/Valet/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Valet\\Tests\\": "tests/"
        }
    },
    "require": {
        "ext-posix": "*",
        "ext-json": "*",
        "php": "^7.4|^8.0|^8.1",
        "illuminate/container": "~5.3|^6.0|^7.0|^8.0|^9.0",
        "mnapoli/silly": "~1.1",
        "symfony/process": "^3.0|^4.0|^5.0|^6.0",
        "nategood/httpful": "~0.2",
        "tightenco/collect": "^5.3|^6.0|^7.0|^8.0",
        "symfony/console": "~5.2"
    },
    "require-dev": {
        "mockery/mockery": "~1.3.3",
        "phpunit/phpunit": "^9.0"
    },
    "scripts": {
        "post-install-cmd": [
        ],
        "post-update-cmd": [
        ]
    },
    "bin": [
        "valet"
    ]
}
